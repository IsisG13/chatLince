{  
  "manifest_version": 3,  
  "name": "WhatsApp Integration",  
  "version": "1.0",  
  "permissions": ["activeTab", "scripting"],  
  "background": {  
    "service_worker": "background.js"  
  },  
  "content_scripts": [  
    {  
      "matches": ["https://web.whatsapp.com/*"],  
      "js": ["content.js"]  
    }  
  ],  
  "web_accessible_resources": [  
    {  
      "resources": ["iframe.html", "iframe.js"],  
      "matches": ["<all_urls>"]  
    }  
  ]  
}