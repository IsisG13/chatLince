{
    "manifest_version": 3,
    "name": "WhatsApp Web Extension",
    "version": "1.0",
    "description": "Extens√£o para adicionar funcionalidades ao WhatsApp Web",
    "permissions": [
      "activeTab",
      "https://web.whatsapp.com/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://web.whatsapp.com/*"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "iframe.html"
            ],
            "matches": [
                "https://web.whatsapp.com/*"
            ]
        }
    ]
  }
  